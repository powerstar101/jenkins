pipeline {
    agent any
    stages{
        stage("clone"){
            steps{
                println "here we clone code"
            }

        }
        stage("build"){
            steps{
                println "here we build code"
            }


        }
        stage("upload artifacts"){
            steps{
                println "here we upload artifacts"
            }


        }
        stage("deploy"){
            steps{
                println "here we  artifacts"
            }


        }
    }
}